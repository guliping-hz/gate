{
  //代理监听端口
  listen_port:5003,
  //代理tcp地址
  address:"127.0.0.1:5002",
  //读超时等待
  read_seconds:60,
  //是否监听websocket
  listen_websocket:true,
  //是否只用有限连接数=连接目标地址
  is_single: false,
  //走指定连接数的时候，网关的唯一ID。确保多个网关ID不冲突。
  single_union_id: 1,
  //需要创建的连接数
  connect_cnt: 10,
  //对外开放地址 ws://127.0.0.1:5003/ws，或者是配置在nginx中的代理地址，如果与默认的服务器ws地址一样可不填，如果配置在新的一台服务器，需配置新地址
  ws_gate: "",
  //ws权重信息
  ws_weight: 0
}